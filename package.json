    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0",
    "terser": "^5.31.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.17"
